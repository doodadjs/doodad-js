{
  "name": "doodad-js",
  "version": "4.0.0",
  "stage": "r",
  "description": "Object-oriented programming framework for Javascript",
  "config": {
    "startup": {
		"fromSource": false,
		"nodeEnvDevValues": "development,dev",
		"enableProperties": false
	},
    "Doodad__Types": {
      "toSourceItemsCount": 255,
      "enableProxies": false,
	  "trapUnhandledRejections": true,
	  "unhandledRejectionsTimeout": 1000,
	  "unhandledRejectionsMaxSize": 20
    },
    "Doodad__Tools": {
      "logLevel": -1
    },
    "Doodad__Tools__Config": {
      "configPath": ""
    },
	"Doodad__Modules": {
		"modulesUri": "./"
	},
    "Doodad": {
        "enforceScopes": false,
        "enforcePolicies": false,
        "publicOnDebug": false
    }
  },
  "files": [
	"tonic.js"
  ],
  "keywords": [
    "oop",
	"interface",
	"interfaces",
	"mixin",
	"mixins",
	"mix-in",
	"mix-ins",
	"class",
	"classes",
	"trait",
	"traits",
    "object",
	"objects",
	"inherit",
	"extend",
	"multiple inheritance",
    "framework",
	"doodad-js"
  ],
  "devDependencies": {
    "npm-package-config": "^0.1.6"
  },
  "tonicExampleFilename": "tonic.js"
}